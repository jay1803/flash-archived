# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'flash (iOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for flash (iOS)
  pod 'WCDB.swift'
end

target 'flash (macOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for flash (macOS)
  pod 'WCDB.swift'
end
